  "dependencies": {
    "connect-redis": "^3.2.0",
    "debug": "^2.6.6",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "oboe": "^2.1.3",
    "redis": "^2.7.1",
    "request": "^2.81.0",
